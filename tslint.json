{
    "rulesDirectory": [
      "node_modules/codelyzer"
    ],
    "rules": {
      "no-console": [
        false,
        "debug",
        "log",
        "time",
        "timeEnd",
        "trace"
      ],
      "new-parens": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": false
    }
}
